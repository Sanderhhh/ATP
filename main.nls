__includes["turtles.nls" "season.nls"]

breed [humans human]

humans-own [
 temp-pref
 humid-pref
 energy
 agression
]

to setup
  clear-all
  set growth-offset [0 0.9 1 0.6]
  set spread-offset [1 0.5 0.9 0.8]
  setup-world

  ask n-of number patches [
    sprout 1
  ]
  reset-ticks
  ask turtles [
    init-turtle
  ]
end

to go
  ask turtles [update-turtles]
  update-patches
  tick
end
